{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestjs-backend-template/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins": [
      "@nestjs/swagger/plugin"
    ],
    "webpack": true,
    "tsConfigPath": "apps/nestjs-backend-template/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nestjs-backend-template",
  "projects": {
    "nestjs-backend-template": {
      "type": "application",
      "root": "apps/nestjs-backend-template",
      "entryFile": "main",
      "sourceRoot": "apps/nestjs-backend-template/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nestjs-backend-template/tsconfig.app.json"
      }
    },
    "images-ms": {
      "type": "application",
      "root": "apps/images-ms",
      "entryFile": "main",
      "sourceRoot": "apps/images-ms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/images-ms/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    }
  }
}